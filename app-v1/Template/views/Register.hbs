          <h2 class="fw-bold mb-5">Sign up now</h2>
          <form onsubmit="/Register" method="post">
            <!-- 2 column grid layout with text inputs for the first and last names -->
            <div class="row">
              <div class="col-md-6 mb-4">
                <div class="form-outline">
                  <input required type="text" id="form3Example1" name="firstName" class="form-control" />
                  <label class="form-label" for="form3Example1">First name</label>
                </div>
              </div>
              <div class="col-md-6 mb-4">
                <div class="form-outline">
                  <input required type="text" id="form3Example2" name="lastName" class="form-control" />
                  <label class="form-label" for="form3Example2">Last name</label>
                </div>
              </div>
            </div>
              {{!-- AGE or Desease --}}
               <div class="row">
              <div class="col-md-6 mb-4">
                <div class="form-outline">
                  <input required type="date" id="form3Example1" name="dob" class="form-control" />
                  <label class="form-label" for="form3Example1">Date of Birth</label>
                </div>
              </div>
              <div class="col-md-6 mb-4">
                <div class="form-outline">
                  <input required type="text" id="form3Example2" name="desease" class="form-control" />
                  <label class="form-label" for="form3Example2">Desease</label>
                </div>
              </div>
            </div>

            <!-- Email input -->
            <div class="form-outline mb-4">
              <input required required type="email" name="email" id="form3Example3" class="form-control" />
              <label class="form-label" for="form3Example3">Email address</label>
            </div>

            <!-- Password input -->
            <div class="form-outline mb-4">
              <input required type="password" minlength="8" id="form3Example4" class="form-control" name="password" />
              <label class="form-label" for="form3Example4">Password</label>
            </div>
            <div class="form-outline mb-4">
              <input required type="checkbox"  name="policy" />
              <label class="form-label" for="form3Example4">Agree to <a href="policy">Terms and Conditions</a></label>
            </div>

        
            <!-- Submit button -->
            <button type="submit" class="btn btn-primary btn-block mb-4">
              Sign up
            </button>

            <!-- Register buttons -->
           
          </form>
